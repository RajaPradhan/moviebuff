{
  "plugins": [
    "syntax-dynamic-import",
    "transform-decorators-legacy",
    "transform-class-properties",
    ["transform-runtime", {
      "regenerator": true
    }]
  ],
  "presets": [
    [
      "env",
      {
        "targets": {
          "chrome": 52,
          "browsers": ["last 2 versions", "safari 7"]
        },
        "modules": false,
        "loose": true
      }
    ],
    "react",
    "airbnb"
  ]
}
